    "vite-plugin-dts": "^1.7.3",
    "vue-tsc": "^1.0.24"
  },
  "scripts": {
    "dev": "vite build && vue-tsc --emitDeclarationOnly",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
